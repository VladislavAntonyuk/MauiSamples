<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             x:Class="MauiBarcode.CommunityToolkitCameraPage">

	<Grid RowDefinitions="*, auto, auto">
		<toolkit:CameraView x:Name="ToolkitCameraView" />
		<Button Grid.Row="1" Text ="Start" Clicked="StartButton_OnClicked"></Button>
		<Button Grid.Row="2" Text ="Stop" Clicked="StopButton_OnClicked"></Button>
	</Grid>

</ContentPage>
